<?xml version="1.0" encoding="utf-8"?>

<manager:PopupBase
    xmlns:manager="clr-namespace:Youtube.Manager"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Youtube.Manager.Views.SearchResult.SearchResult"
    xmlns:controls="clr-namespace:Youtube.Manager.Controls"
    xmlns:template="clr-namespace:Youtube.Manager.Views.Template">
    <StackLayout>
        <StackLayout BackgroundColor="{StaticResource applicationColor}">
            <template:VideoItem x:Name="stVideoContainer" OnItemClick="StVideoContainer_OnItemClick" HeaderIsVisible="False" />
        </StackLayout>
    </StackLayout>
</manager:PopupBase>